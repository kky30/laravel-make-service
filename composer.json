{
    "name": "kky30/laravel-make-service",
    "description": "A simple package to create a make:service command for Laravel 9+",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "kky30",
            "email": "nonefishy@gmail.com"
        }
    ],

    "minimum-stability": "dev",

    "autoload": {
        "psr-4": {
            "Kky30\\LaravelMakeService\\": "src/"
        },
        "classmap": ["src/"]
    },

    "require": {
        "php": "^8.1"
    },

    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0"
    },

    "extra": {
        "laravel": {
            "providers": [
                "Kky30\\LaravelMakeService\\MakeServiceProvider"
            ]
        }
    }
}
